{
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html"
                ],
                "versionedFiles": [
                    "/*.bundle.css",
                    "/*.bundle.js",
                    "/*.chunk.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "rss2json",
            "urls": [
                "https://rss2json.com/**"
            ],
            "cacheConfig": {
                "maxSize": 100,
                "maxAge": "1d",
                "strategy": "freshness"
            }
        }
    ]
}